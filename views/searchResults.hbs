<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="stylesheet" href="/stylesheets/style.css" />

    <title>North Shore Booch</title>
  </head>
  <body id="searchResBod">
    <main>
      {{#if message}}
      <div id="searchMsg">
      <img class="mb-4 d-block mx-auto" src="/images/logo.png" height="72" />
          {{message}}
      </div>
      {{/if}}

    {{#each products}}
    {{>tempProduct product=this}}
    {{/each}}

    {{#each events}}
    {{> tempEvents event=this}}
    {{/each}}
       
    </main>

  </body>
</html>